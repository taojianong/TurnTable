package com.math {
	
	/**
	 * 3D Point (x,y,z)
	 * @author taojianlong 2015/3/9 13:47
	 */
	public class Point3D {
		
		public var x:Number;
		public var y:Number;
		public var z:Number;
		
		public function Point3D( x:Number = 0 , y:Number = 0 , z:Number = 0 ) {
			
			this.x = x;
			this.y = y;
			this.z = z;
		}	
	}
}